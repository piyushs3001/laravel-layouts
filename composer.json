{
    "name": "piyush/laravel-layouts",
    "description": "A Laravel package for creating custom layout",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Piyush",
            "email": "piyushs.spacei@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Piyush\\LaravelLayouts\\": "src/"
        }
